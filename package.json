    "axios": "^1.1.3",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.5",
    "joi": "^17.6.3",
    "mongoose": "^6.6.6",
    "next": "12.3.1",
    "next-i18next": "^12.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.38.0",
